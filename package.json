{
  "name": "contendo-business-platform",
  "version": "1.0.0",
  "description": "Contendo Business Management Platform - Unified dashboard for healthcare, training, Arbiter, CRM, and financial management",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run build:client",
    "build:client": "cd src/client && npm run build",
    "start": "node dist/index.js",
    "dev": "concurrently \"ts-node src/index.ts\" \"cd src/client && npm run dev\"",
    "dev:server": "ts-node src/index.ts",
    "dev:client": "cd src/client && npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docker:build": "docker build -t contendo-platform .",
    "docker:run": "docker-compose up"
  },
  "keywords": ["business", "management", "crm", "accounting", "dashboard"],
  "author": "Contendo",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "@supabase/supabase-js": "^2.39.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "axios": "^1.6.2",
    "express-session": "^1.17.3",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "jsonwebtoken": "^9.0.2",
    "tesseract.js": "^5.0.4",
    "openai": "^4.20.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "@types/jest": "^29.5.8",
    "@types/express-session": "^1.17.10",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
